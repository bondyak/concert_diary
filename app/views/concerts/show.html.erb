<div class="page-header">
  <h3>Concert #<%= @concert.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Date</dt>
      <dd><%= @concert.date %></dd>

      <dt>Notes</dt>
      <dd><%= @concert.notes %></dd>

      <dt>Artist</dt>
      <dd><%= @concert.artist_id %></dd>

      <dt>Venue</dt>
      <dd><%= @concert.venue_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/concerts" class="btn btn-primary">
        Back
      </a>
      <a href="/concerts/<%= @concert.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_concert/<%= @concert.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
